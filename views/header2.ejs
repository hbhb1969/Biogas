        <div class="biogas">
          BIOGAS
        </div>
        <div class="nsm">
          Nährstoffmanager
        </div>
      </div>
    </a>
    <nav>
      <div class="navbar">
        <ul class="nav">
          <li><a href="/abmelden"><img src="\bilder\abmelden.png"></a></li>
          <li><a href="/hauptmenue"><img src="\bilder\menue.png"></a></li>
        </ul>
      </div>
    </nav>
  </div>
  <div class="ueberschrift-balken buchungen grau">
    <div class="ueberschrift-balken-inhalt">
      <% if (headerTitel.length > 0) { %><%= headerTitel %><% } %><img src="\bilder\<% if (headerBild.length > 0) { %><%= headerBild %><% } %>">
    </div>
  </div>
